{
  "name": "demo-app",
  "private": true,
  "version": "0.3.0",
  "description": "JrFS demo frontend app.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check-types": "tsc",
    "clean": "shx rm -rf dist/*",
    "copy-dist": "shx mkdir -p ../dev-server/public && shx cp -r dist/* ../dev-server/public",
    "generate-api": "openapi-typescript http://localhost:40140/api/v1/docs/json -o ./src/platform/client/api.gen.ts",
    "format": "prettier --write \"./src/**/*.{css,js,jsx,scss,ts}\"",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --ext mdx,ts,tsx --fix --max-warnings 0",
    "lint-ts": "tsc --noEmit --pretty",
    "preview": "npm run build && vite preview",
    "start": "vite",
    "start-lan": "vite --host 0.0.0.0",
    "watch": "tsc --noEmit -w"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.9.0",
    "@jrfs/idb": "*",
    "@jrfs/typebox": "*",
    "@jrfs/web": "*",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@sinclair/typebox": "^0.33.7",
    "clsx": "^2.1.1",
    "joi": "^17.13.3",
    "lodash.debounce": "^4.0.8",
    "mutative": "^1.0.11",
    "openapi-fetch": "^0.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "tinybase": "^5.3.1",
    "tinykeys": "^3.0.0",
    "valtio": "^2.0.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "openapi-typescript": "^7.4.1",
    "sass": "^1.79.4",
    "vite": "^5.4.8"
  }
}

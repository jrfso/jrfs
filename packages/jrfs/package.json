{
  "name": "jrfs",
  "version": "0.0.1",
  "description": "JRFS (JSON and Resources File System) is a (transactional, queryable, websocket accessible, collaborative, caching) file system library with customizable driver and file type providers.",
  "repository": "github:jrfso/jrfs",
  "keywords": [
    "json",
    "fs",
    "filesystem",
    "websocket",
    "websockets"
  ],
  "license": "MIT",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/",
    "./*": "./lib/*",
    "./core": "./lib/core/index.js",
    "./nanoid": "./lib/nanoid/index.js",
    "./node": "./lib/node/index.js",
    "./typebox": "./lib/typebox/index.js",
    "./web": "./lib/web/index.js",
    "./ws": "./lib/ws/index.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "run-s rm-build build-esm",
    "build-esm": "node ../../scripts/build-esm.mjs lib --tsconfig tsconfig.prod.json",
    "build-ts": "tsc --project ./tsconfig.eslint.json --noEmit",
    "format": "prettier --write \"./**/*.{css,js,jsx,scss,ts}\"",
    "lint": "eslint .",
    "lint-fix": "eslint . --ext .ts --fix",
    "lint-ts": "tsc --noEmit --pretty --project ./tsconfig.eslint.json",
    "rm-build": "shx rm -rf lib/*",
    "watch-build": "node --watch-path=./src ../../scripts/build-esm.mjs lib --tsconfig tsconfig.prod.json"
  },
  "dependencies": {
    "mutative": "1.0.11"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.33.7",
    "dequal": "^2.0.3",
    "jsonpath-plus": "^9.0.0",
    "mutative": "1.0.11",
    "nanoid": "^5.0.7",
    "object-hash": "^3.0.0"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.33.7",
    "nanoid": "^5.0.7"
  },
  "peerDependenciesMeta": {
    "@sinclair/typebox": {
      "optional": true
    },
    "nanoid": {
      "optional": true
    }
  }
}